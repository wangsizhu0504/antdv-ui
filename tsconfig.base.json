{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": ".",
    "rootDir": ".",
    "module": "ESNext",
    "moduleResolution": "node",
    "paths": {
      "@antdv/*": ["packages/*"]
    },
    "resolveJsonModule": true,
    "types": [],
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist",
    "sourceMap": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true
  }
}
