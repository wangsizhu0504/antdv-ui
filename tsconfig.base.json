{
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "dist",
    "baseUrl": ".",
    "sourceMap": false,
    "moduleResolution": "node",
    "target": "es6",
    "module": "ESNext",
    "strictNullChecks": false,
    "isolatedModules": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "types": []
  }
}
