{
  "name": "@antdv/ui",
  "version": "4.2.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Fork from Ant Design Vue. An enterprise-class UI design language and Vue-based implementation",
  "license": "MIT",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/ant-design-vue"
  },
  "homepage": "https://www.antdv.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vueComponent/ant-design-vue.git"
  },
  "bugs": {
    "url": "https://github.com/vueComponent/ant-design-vue/issues"
  },
  "keywords": [
    "vue",
    "vue3",
    "vue3-components",
    "ant-design-vue",
    "ant-design",
    "antd",
    "vueComponent",
    "component",
    "components",
    "ui",
    "framework",
    "frontend"
  ],
  "sideEffects": [
    "dist/*"
  ],
  "web-types": "./web-types.json",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./global": {
      "types": "./volar.d.ts"
    },
    "./es": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./lib": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.cjs"
    },
    "./es/*.mjs": {
      "types": "./es/*.d.ts",
      "import": "./es/*.mjs"
    },
    "./es/*": {
      "types": [
        "./es/*.d.ts",
        "./es/*/index.d.ts"
      ],
      "import": "./es/*.mjs"
    },
    "./lib/*.cjs": {
      "types": "./lib/*.d.ts",
      "require": "./lib/*.cjs"
    },
    "./lib/*": {
      "types": [
        "./lib/*.d.ts",
        "./lib/*/index.d.ts"
      ],
      "require": "./lib/*.cjs"
    },
    "./*": "./*"
  },
  "main": "lib/index.cjs",
  "module": "es/index.mjs",
  "unpkg": "dist/index.full.js",
  "jsdelivr": "dist/index.full.js",
  "types": "es/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "es",
    "lib"
  ],
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "@ant-design/colors": "^7.2.0",
    "@ant-design/icons-vue": "^7.0.1",
    "@ctrl/tinycolor": "^4.1.0",
    "@emotion/hash": "^0.9.2",
    "@emotion/unitless": "^0.10.0",
    "async-validator": "^4.2.5",
    "csstype": "3.1.3",
    "dayjs": "^1.11.13",
    "dom-align": "^1.12.4",
    "lodash-es": "^4.17.21",
    "resize-observer-polyfill": "^1.5.1",
    "scroll-into-view-if-needed": "^3.1.0",
    "stylis": "^4.3.5",
    "vue-types": "^5.1.3"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^2.1.8"
  }
}
